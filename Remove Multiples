#include <iostream>
using namespace std;

int main() {
    int t;
    cin >> t;
    while (t--) {
        long long n, m, sum = 0;
        cin >> n >> m;
        for (int i = 0; i < m; i++) {
            long long x;
            cin >> x;
            sum += x;
        }
        cout << (n * (n + 1)) / 2 - sum << endl;
    }
    return 0;
}
